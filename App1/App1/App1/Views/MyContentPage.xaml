<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.Views.MyContentPage">

  
  <Grid>
  
    <Grid.RowDefinitions>
      <RowDefinition Height="2*"/>
      <RowDefinition Height="2*"/>
      <RowDefinition Height="2*"/>
      <RowDefinition Height="2*"/>
    </Grid.RowDefinitions>
    
    <Button Grid.Row="0"
            Text="StackLayout"
            Clicked="StackLayout_Clicked"/>

    <Button Grid.Row="1"
            Text="Página de Componentes"
            Clicked="MasterDetail_Clicked"/>

    <Button Grid.Row="2"
            Text="ListView"
            Clicked="ListView_Clicked"/>

    <Button Grid.Row="3"
        Text="Picker"
        Clicked="Picker_Clicked"/>
  
  </Grid>
  
</ContentPage>